{
  "name": "punypage",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"frontend,backend\" -c \"cyan,magenta\" \"bun run dev:frontend\" \"bun run dev:server\"",
    "dev:frontend": "vite --port 3000",
    "dev:server": "cd server && bun --watch src/index.ts",
    "build": "vite build && tsc",
    "build:server": "tsc -p server/tsconfig.json",
    "serve": "vite preview",
    "start:server": "cd server && bun dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fastify": "^5.6.2",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "jsdom": "^27.0.0",
    "pino-pretty": "^13.1.2",
    "supabase": "^2.58.5",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0"
  }
}
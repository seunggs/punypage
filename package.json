{
  "name": "punypage",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n \"frontend,backend\" -c \"cyan,magenta\" \"bun run dev:frontend\" \"bun run dev:server\"",
    "dev:frontend": "VITE_API_URL=$(bun scripts/get-port.ts backend-url) vite --port $(bun scripts/get-port.ts frontend)",
    "dev:server": "export PORT=$(bun scripts/get-port.ts backend) && export FRONTEND_URL=$(bun scripts/get-port.ts frontend-url) && cd server && uv run uvicorn app.main:app --reload --host 0.0.0.0 --port $PORT",
    "build": "vite build && tsc",
    "build:server": "echo 'No build step needed for Python'",
    "serve": "vite preview",
    "start:server": "cd server && uv run uvicorn app.main:app --host 0.0.0.0 --port ${PORT:-4000}",
    "test": "vitest run"
  },
  "dependencies": {
    "@fontsource/geist-mono": "^5.2.7",
    "@fontsource/geist-sans": "^5.2.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@tiptap/core": "3.13.0",
    "@tiptap/extension-bubble-menu": "^3.13.0",
    "@tiptap/extension-placeholder": "3.13.0",
    "@tiptap/extension-typography": "^3.13.0",
    "@tiptap/markdown": "^3.13.0",
    "@tiptap/pm": "3.13.0",
    "@tiptap/react": "3.13.0",
    "@tiptap/starter-kit": "3.13.0",
    "@tiptap/static-renderer": "^3.13.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.1",

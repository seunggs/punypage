    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.3.6",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
